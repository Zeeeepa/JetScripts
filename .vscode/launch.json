        // "PYTHONPATH": "/Users/jethroestrada/.pyenv/versions/3.12.7/lib/python3.12/site-packages",
        "PYTHONPATH": "/Users/jethroestrada/Desktop/External_Projects/JetScripts/.venv/lib/python3.12/site-packages:$PYTHONPATH"
      }
    },
    {
      "name": "Jet Scripts:  Uvicorn - Neural Search",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/vectors/qdrant/neural-search",
      "args": [
        "service:app",
        "--reload",
        "--host",
        "0.0.0.0",
