{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}",
      "python": "/Users/jethroestrada/.pyenv/shims/python",
      "env": {
        "PYTHONPATH": "/Users/jethroestrada/Desktop/External_Projects/jet_python_modules"
      }
    },
    {
      "name": "Jet Scripts: Python Debugger",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "python": "/Users/jethroestrada/.pyenv/shims/python",
      "cwd": "${workspaceFolder}",
      "env": {
        "SENTENCE_TRANSFORMERS_HOME": "/Volumes/jethroestrada/.cache/huggingface/hub",
        // "RAG_EMBEDDING_MODEL_AUTO_UPDATE": "true",
        // "RAG_EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2",
        "RAG_EMBEDDING_ENGINE": "ollama",
